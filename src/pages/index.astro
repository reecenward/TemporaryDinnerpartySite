---
import Layout from "@/layouts/Layout.astro";
import LandingVideo from "@/components/home/landing-video.astro";
import ContentSlider from "@/components/home/content-slider.astro";
import BigLogo from "@/components/home/big-logo.astro";

import About from "@/components/home/about.astro"
import Faq from "@/components/home/faq.astro"
import WhatsIncluded from "@/components/home/whats-inside.astro";
import Cta from "@/components/home/cta.astro";
import Brief from "@/components/home/breif.astro"
import IsThisForMe from "@/components/home/Is-this-for-me.astro";
import '@/styles/globals.css'
---

<Layout
  title="Dinnerparty - Vancouver Organic Social Media Marketing Studio"
  description="At Dinnerparty we create short-form video content that helps brands grow their community and connect with their audience organically."
  ogTitle="Dinnerparty - Vancouver Organic Social Media Marketing Studio"
  ogDescription="At Dinnerparty we create short-form video content that helps brands grow their community and connect with their audience organically."
  ogType="website"
  ogUrl="https://dinnerparty.studio"
  ogImage={"/boot.png"}
>
  <meta
    name="keywords"
    content="Vancouver, Social media marketing, Dinnerparty studio"
  />
  <main >
    <BigLogo />
    <ContentSlider />
    <LandingVideo />
    <Brief/>
    <WhatsIncluded/>
    <!-- <IsThisForMe/> -->
    <About/>

    <Faq/>
    <Cta/>
  </main>
</Layout>
